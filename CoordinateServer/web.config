<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>

    <rewrite>
      <rules>
        <rule name="CoordinateServer">
          <match url="/*" />
          <action type="Rewrite" url="build/server.js" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>